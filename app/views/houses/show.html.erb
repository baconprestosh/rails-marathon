<h1><%= @house.name %></h1>
<h1><%= @house.author %></h1>
<h1><%= @house.source %></h1>
<h1><%= @house.motto %></h1>
<% @house.members.each do |member| %>
<%= member.first_name %>
<%= member.last_name %>
<%= member.created_at.strftime("%B %d %Y") %>
<% end %>

<%= link_to "Add a House Member", new_house_member_path(@house) %>
